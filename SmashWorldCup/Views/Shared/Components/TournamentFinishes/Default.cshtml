@model List<SmashWorldCup.Models.CharacterModel>

@{
    var titles = TempData["WinnerTitles"] + " title";
}

@if(Model.Count != 0)
{
    <div>
        <p>
            Winner: @Model[0].Name (@titles) <br>
            Runner Up: @Model[1].Name <br>
            Third Place: @Model[2].Name <br>
            Fourth Place: @Model[3].Name <br>
        </p>
    </div>
}